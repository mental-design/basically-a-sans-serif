    AE.smcp OE.smcp ETH.smcp THORN.smcp ENG.smcp SCHWA.smcp ESZETT.smcp
];

# Numerics
@numerics = [
    zero one two three four five six seven eight nine
    one.ss01 three.ss01 eight.ss01
];
@tnums = [
    zero.tnum one.tnum two.tnum three.tnum four.tnum
    five.tnum six.tnum seven.tnum eight.tnum nine.tnum
    one.ss01.tnum three.ss01.tnum eight.ss01.tnum
];

@sup_num = [
    sup_zero sup_one sup_two sup_three sup_four
    sup_five sup_six sup_seven sup_eight sup_nine
    sup_one.ss01 sup_three.ss01 sup_eight.ss01
];
@sub_num = [
    sub_zero sub_one sub_two sub_three sub_four
    sub_five sub_six sub_seven sub_eight sub_nine
    sub_one.ss01 sub_three.ss01 sub_eight.ss01
];

@figuresNumeratorEx = [fraction @sup_num];
@figuresDenominatorEx = [fraction @sub_num];

# Operators
@braces = [parenleft parenright bracketleft bracketright braceleft braceright];
@operators = [
    hyphen plus minus multiply divide plusminus 
    equal less greater almost_equal not_equal less_equal greater_equal
    divide.ss02
];

@eyes = [colon semicolon colon.ss02 semicolon.ss02];

