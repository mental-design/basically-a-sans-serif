#
# Lookups
#
lookup posops {
    pos @numerics @operators' @numerics <0 60 0 0>;
    pos @numerics @operators' space @numerics <0 60 0 0>;
    pos @numerics space @operators' @numerics <0 60 0 0>;
    pos @numerics space @operators' space @numerics <0 60 0 0>;

    pos @tnums @operators' @tnums <0 60 0 0>;
    pos @tnums @operators' space @tnums <0 60 0 0>;
    pos @tnums space @operators' @tnums <0 60 0 0>;
    pos @tnums space @operators' space @tnums <0 60 0 0>;

    pos @braces @operators' @braces <0 60 0 0>;
    pos @braces @operators' space @braces <0 60 0 0>;
    pos @braces space @operators' @braces <0 60 0 0>;
    pos @braces space @operators' space @braces <0 60 0 0>;
} posops;

lookup smile {
    pos @eyes' hyphen <0 150 0 0>;
    pos hyphen @eyes' <0 150 0 0>;

    pos @eyes hyphen' <0 60 0 0>;
    pos hyphen' @eyes <0 60 0 0>;
} smile;

lookup subops {
    sub @numerics space hyphen' space @numerics by minus;
    sub @numerics plus' hyphen' @numerics by plusminus;
    sub @numerics plus' hyphen' space @numerics by plusminus;
    sub @numerics space plus' hyphen' @numerics by plusminus;
    sub @numerics space plus' hyphen' space @numerics by plusminus;
} subops;

lookup IJAcute {
    sub I_with_acute J' by J_with_acute;
    sub i_with_acute j' by j_with_acute;
} IJAcute;

#
# LOCL
#
feature locl {
    script latn;
    language NLD;

    lookup IJAcute;
} locl;

#
# Alternatives
#
feature aalt {
    # feature calt;

    feature smcp;
    feature unic;

    feature tnum;
    feature pnum;

    feature ss01;
    feature ss02;
    feature ss03;
    # feature ss04;
} aalt;

feature calt {
    lookup posops;
    lookup subops;
    lookup smile;
    
    sub @numerics colon' @numerics by raised_colon;
} calt;

feature frac {
    sub @numerics slash' @numerics by fraction;
    rsub @numerics' @figuresNumeratorEx by @sup_num;
    sub @figuresDenominatorEx @numerics' by @sub_num;
    sub @numerics space' @figuresNumeratorEx by space.frac;
} frac;

#
# CCMP - Glyph Composition / Decomposition
#
lookup i_sub {
    sub i' [combining_vertline combining_vertlinedbl] by i_dotless;
} i_sub;

feature ccmp {
    lookup i_sub;
} ccmp;

#
# Numerics
#
feature pnum {
    sub @tnums by @numerics;
} pnum;

feature tnum {
    sub @numerics by @tnums;
} tnum;

#
# Case
#
feature smcp {
    sub @lowercase by @smallCaps;
    sub @lowercaseDiacritics by @smallCapsDiacritics;
} smcp;

feature unic {
